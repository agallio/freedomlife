{
  "name": "freedomlife",
  "version": "4.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn lint && yarn type-check"
  },
  "engines": {
    "node": "12.x.x",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@splitbee/web": "^0.2.4",
    "@tailwindcss/typography": "^0.4.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "framer-motion": "^3.2.1",
    "gray-matter": "^4.0.2",
    "mongodb": "^3.6.3",
    "next": "^10.0.5",
    "next-offline": "^5.0.3",
    "next-seo": "^4.17.0",
    "next-themes": "^0.0.10",
    "preact": "^10.5.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-toast": "^1.0.2",
    "react-media-hook": "^0.4.9",
    "react-modal-sheet": "^1.2.0",
    "remark": "^13.0.0",
    "remark-html": "^13.0.1",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.2.1",
    "@types/mongodb": "^3.6.3",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "autoprefixer": "10.2.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-react": "^7.22.0",
    "globby": "^11.0.2",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "postcss": "8.2.4",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.2",
    "typescript": "^4.1.3"
  },
  "license": "MIT"
}
