{
  "name": "freedomlife",
  "author": "agallio",
  "license": "MIT",
  "version": "3.1.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn lint && yarn type-check"
  },
  "engines": {
    "node": "12.x.x || 14.x.x",
    "npm": "please-use-yarn",
    "yarn": ">= 1.22.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run test-all"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/styles": "^4.11.2",
    "axios": "^0.21.1",
    "dayjs": "^1.9.8",
    "mongoose": "^5.11.9",
    "next": "latest",
    "next-offline": "^5.0.3",
    "next-seo": "^4.17.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "sass": "^1.32.0",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.17",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-react": "^7.22.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
